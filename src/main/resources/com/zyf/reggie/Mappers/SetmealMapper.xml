<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zyf.reggie.Mappers.SetmealMapper">
    <select id="count" resultType="java.lang.Integer">
        select count(category_id) from setmeal where category_id =#{id}
    </select>
    <select id="selectByCategoryId" resultType="com.zyf.reggie.pojo.Setmeal">
        select * from setmeal where category_id=#{categoryId} and status=#{status};
    </select>
    <select id="selectAll" resultType="com.zyf.reggie.pojo.Setmeal">
        select * from setmeal
    </select>

</mapper>